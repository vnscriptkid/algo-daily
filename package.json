  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel . --extensions .js,.ts --out-dir dist",
    "test": "jest",
    "lint": "eslint . --fix --ext .js",
    "prettier": "prettier \"**/*.+(js|json|ts)\"",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel lint check-format",
